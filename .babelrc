{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "chrome": "91",
            "firefox": "90",
            "safari": "15",
            "edge": "91"
          },
          "bugfixes": true,
          "modules": false,
          "useBuiltIns": false
        }
      }
    ]
  ],
  "plugins": [],
  "env": {
    "production": {
      "plugins": [
        ["transform-remove-console", { "exclude": ["error", "warn"] }]
      ]
    }
  }
}
